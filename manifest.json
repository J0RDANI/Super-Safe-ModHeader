{
    "manifest_version": 3,
    "name": "Super Safe ModHeader",
    "version": "1.0.1",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
      },
    "host_permissions": ["<all_urls>"],
    "permissions": [
      "declarativeNetRequest",
      "activeTab"
    ]
  }
  